   <soapenv:Header>
      <urn:LogicalAddress></urn:LogicalAddress>
   </soapenv:Header>
   <soapenv:Body>
      <urn1:ListPossibleReceivers>
         <urn1:intygTyp>
            <urn2:code>$intygTyp$</urn2:code>
            <urn2:codeSystem></urn2:codeSystem>
         </urn1:intygTyp>
         <!--You may enter ANY elements at this point-->
      </urn1:ListPossibleReceivers>
   </soapenv:Body>
</soapenv:Envelope>
>>
