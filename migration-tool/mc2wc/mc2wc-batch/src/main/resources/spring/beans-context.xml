<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="certificateConverterProcessor"
        class="se.inera.certificate.mc2wc.batch.processors.CertificateConverterProcessor" />

  <bean id="migrationMessageConverter" class="se.inera.certificate.mc2wc.converter.MigrationMessageConverterImpl" />

  <bean id="migrationMessageValidationProcessor"
        class="se.inera.certificate.mc2wc.batch.processors.MigrationMessageValidationProcessor" />

  <bean id="migrationMessageSchemaValidator"
        class="se.inera.certificate.mc2wc.schema.MigrationMessageSchemaValidatorImpl" init-method="init" />

</beans>